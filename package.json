{
  "private": true,
  "name": "starter-lib",
  "version": "0.10.0",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "lint": "eslint --ext .js ./",
    "lint:critical": "eslint --no-eslintrc --config .eslintrc.critical.json ./",
    "format": "prettier './**/*.{md,json,js,css,scss}' --write && eslint -o /dev/null --ext .js --fix ./ || true",
    "test": "NODE_ICU_DATA=node_modules/full-icu jest",
    "build": "babel --source-maps true ./src/ -d ./dist/",
    "build:watch": "babel -w --source-maps true ./src/ -d ./dist/"
  },
  "dependencies": {
    "@babel/polyfill": "~7.4.3",
    "debug": "~4.1.1",
    "events": "~3.0.0",
    "intl": "~1.2.5",
    "intl-relativeformat": "~2.1.0",
    "whatwg-fetch": "~3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.4.3",
    "@babel/core": "~7.4.3",
    "@babel/preset-env": "~7.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "~10.0.1",
    "babel-jest": "~24.7.1",
    "eslint": "~5.16.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-plugin-import": "~2.16.0",
    "full-icu": "~1.2.1",
    "jest": "~24.7.1",
    "prettier": "~1.17.0"
  }
}
