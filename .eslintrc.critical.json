{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-undef": "error",
    "no-redeclare": "error",
    "no-unused-vars": "warn",
    "no-invalid-this": "error"
  }
}
